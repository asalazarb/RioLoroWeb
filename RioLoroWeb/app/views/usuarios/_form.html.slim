= form_with(model: usuario, local: true) do |form|
  - if usuario.errors.any?
    #error_explanation
      h2
        = pluralize(usuario.errors.count, "error")
        |  prohibited this usuario from being saved:
      ul
        - usuario.errors.full_messages.each do |message|
          li
            = message
  .field
    = form.label :nombre
    = form.text_field :nombre, id: :usuario_nombre
  .field
    = form.label :apellido1
    = form.text_field :apellido1, id: :usuario_apellido1
  .field
    = form.label :apellido2
    = form.text_field :apellido2, id: :usuario_apellido2
  .field
    = form.label :cedula
    = form.text_field :cedula, id: :usuario_cedula
  .field
    = form.label :nombreUsuario
    = form.text_field :nombreUsuario, id: :usuario_nombreUsuario
  .field
    = form.label :password
    = form.text_field :password, id: :usuario_password
  .actions
    = form.submit
